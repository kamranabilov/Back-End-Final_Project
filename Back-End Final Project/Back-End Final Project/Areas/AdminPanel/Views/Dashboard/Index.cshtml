@model List<ContactUs>
@{
    ViewData["Title"] = "Index";
}
<div class="main-panel">   
    <div class="table-responsive pt-3">
        <table class="table table-success">
            <thead>
                <tr>
                    <th>
                        Name
                    </th>
                    <th>
                        Email
                    </th>
                    <th>
                       Subject
                    </th>
                    <th>
                        Message
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr style="background-color:blue" >
                        <td >
                            @item.Name
                        </td>
                        <td>
                            @item.Email
                        </td>
                        <td>
                            @item.Subject
                        </td>
                        <td>
                            @item.Message
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>


